{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "fs:default",
    "fs:allow-stat",
    "dialog:default",
    "shell:allow-execute",
    {
      "identifier": "fs:allow-read",
      "allow": [{ "path": "**" }]
    }
  ]
}
